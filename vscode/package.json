{
    "name": "shiro-language",
    "displayName": "Shiro Language",
    "description": "Provides syntax highlighting for Shiro",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.65.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "shiro",
                "aliases": [
                    "shiro",
                    "shiro"
                ],
                "extensions": [
                    "shiro"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "shiro",
                "scopeName": "source.shiro",
                "path": "./syntaxes/shiro.tmLanguage.json"
            }
        ]
    }
}
